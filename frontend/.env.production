# TurtleTrading Frontend Environment Variables - Production

# API Configuration
VITE_API_BASE_URL=https://api.turtletrading.com
VITE_API_TIMEOUT=30000

# WebSocket Configuration
VITE_WS_BASE_URL=wss://api.turtletrading.com

# Application Configuration
VITE_APP_NAME=TurtleTrading
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG_LOGGING=false
VITE_ENABLE_PERFORMANCE_MONITORING=true

# External Services (Set these in deployment environment)
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_HOTJAR_ID=

# Market Data Configuration
VITE_DEFAULT_SYMBOLS=AAPL,MSFT,NVDA,GOOGL,META,AMZN,TSLA,JPM,QQQ,SPY
VITE_DEFAULT_REFRESH_INTERVAL=3000

# UI Configuration
VITE_DEFAULT_THEME=system
VITE_DEFAULT_CURRENCY=USD
VITE_DEFAULT_TIMEZONE=America/New_York